<div class="form-group">
    <button class="btn btn-primary" onclick="changePassword();">Đổi mật khẩu</button>
</div>
<div class="form-group">
    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">Email</span>
        </div>
        <input type="text" readonly class="form-control" value="<%= user.email %>">
    </div>
</div>
<div class="form-group">
    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">Tên</span>
        </div>
        <input type="text" readonly class="form-control" value="<%= user.name %>">
    </div>
</div>
<div class="form-group">
    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">Số ĐT</span>
        </div>
        <input type="text" id="phone" class="form-control" value="<%= user.phone %>">
    </div>
</div>
<div class="form-group">
    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">Giới tính</span>
        </div>
        <input type="text" id="gender" class="form-control" value="<%= user.gender %>">
    </div>
</div>
<div class="form-group">
    <div class="input-group">
        <div class="input-group-prepend">
            <span class="input-group-text">Tuổi</span>
        </div>
        <input type="text" id="age" class="form-control" value="<%= user.age %>">
    </div>
</div>